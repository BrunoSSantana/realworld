{
  "name": "nodejs-graphql-fp",
  "version": "0.1.0",
  "main": "index.js",
  "repository": "git@github.com:fdaciuk/nodejs-graphql-fp.git",
  "author": "Fernando Daciuk - @fdaciuk",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node dist/src/index.js",
    "start:dev": "nodemon --exec sucrase-node src/index.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:integration": "vitest --passWithNoTests --config jest.config.integration.js",
    "test:integration:watch": " npm run test:integration --watch",
    "lint": "eslint . --ext js,ts",
    "lint:fix": "eslint --fix . --ext js,ts",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "prepare": "husky install",
    "build": "tsc",
    "ci": "npm run lint && npm run type-check && npm run test"
  },
  "dependencies": {
    "argon2": "0.29.1",
    "cors": "2.8.5",
    "dotenv": "16.0.1",
    "express": "^4.18.1",
    "fp-ts": "2.12.2",
    "io-ts": "^2.2.17",
    "io-ts-types": "^0.5.16",
    "jose": "4.9.2",
    "module-alias": "2.2.2",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "slugify": "1.6.5",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.7",
    "@types/module-alias": "2.0.1",
    "@types/node": "18.7.8",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "@vitest/coverage-c8": "^0.23.2",
    "eslint": "^8.22.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "26.8.4",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "nodemon": "2.0.19",
    "sucrase": "3.25.0",
    "ts-jest": "28.0.8",
    "typescript": "4.7.4",
    "vitest": "^0.23.2"
  }
}
