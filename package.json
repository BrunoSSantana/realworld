{
  "name": "nodejs-graphql-fp",
  "version": "0.1.0",
  "main": "index.js",
  "repository": "git@github.com:fdaciuk/nodejs-graphql-fp.git",
  "author": "Fernando Daciuk - @fdaciuk",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "nodemon --exec sucrase-node src/index.ts",
    "test": "jest --passWithNoTests && yarn test:integration",
    "test:watch": "jest --watch",
    "test:integration": "jest --passWithNoTests --config jest.config.integration.js",
    "test:integration:watch": " yarn test:integration --watch",
    "lint": "eslint . --ext js,ts",
    "lint:fix": "eslint --fix . --ext js,ts",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "prepare": "husky install",
    "build": "tsc",
    "ci": "yarn lint && yarn type-check && yarn test"
  },
  "dependencies": {
    "dotenv": "16.0.1",
    "fp-ts": "2.12.2",
    "module-alias": "2.2.2"
  },
  "devDependencies": {
    "@types/jest": "28.1.7",
    "@types/module-alias": "2.0.1",
    "@types/node": "18.7.8",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "eslint": "^8.22.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "26.8.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "nodemon": "2.0.19",
    "sucrase": "3.25.0",
    "ts-jest": "28.0.8",
    "typescript": "4.7.4"
  }
}
